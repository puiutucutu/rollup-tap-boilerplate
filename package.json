{
  "name": "tape-rollup-boilerplate",
  "description": "a sample setup of tape for unit testing with code coverage and bundling with rollup",
  "version": "1.0.0",
  "main": "dist/tape-rollup-boilerplate.cjs.js",
  "module": "dist/tape-rollup-boilerplate.esm.js",
  "browser": "dist/tape-rollup-boilerplate.umd.js",
  "scripts": {
    "example:cjs": "node ./example/node-cjs.js",
    "example:esm": "node -r esm ./example/node-esm.js",
    "example:umd": "node ./example/node-umd.js",
    "build": "cross-env BABEL_ENV=build rollup -c",
    "coverage": "cross-env BABEL_ENV=test nyc tape ./test/**/*.js",
    "test": "cross-env BABEL_ENV=test tape -r @babel/register ./test/**/*.js | tap-spec",
    "prepublish": "npm run test && npm run build",
    "release": "np"
  },
  "dependencies": {
    "cross-env": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "babel-plugin-istanbul": "^5.1.3",
    "esm": "^3.2.25",
    "np": "^5.1.0",
    "nyc": "^14.0.0",
    "rollup": "^1.10.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-terser": "^4.0.4",
    "tap-spec": "^5.0.0",
    "tape": "^4.10.1"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git"
  }
}
